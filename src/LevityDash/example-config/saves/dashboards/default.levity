- type: graph
  timeframe:
    days: 2
    hours: 18
  annotations:
    dayLabels:
      alignment: TopCenter
      opacity: 20%
    hourLabels:
      alignment: BottomCenter
      opacity: 100%
    lines:
      weight: 0.3
  geometry:
    height: 63.9%
    width: 61.9%
    x: 37.8%
    y: 0%
  figures:
    - figure: temperature
      margins:
        bottom: 22.1%
        top: 25.5%
      environment.temperature.dewpoint:
        plot:
          type: plot
          cap: square
          color: #5882ff
          dashPattern: 5, 5
          weight: 0.322
      environment.temperature.feelsLike:
        plot:
          type: plot
          cap: round
          gradient: TemperatureGradient
          weight: 0.6
        resolution: 2
      environment.temperature.temperature:
        labels:
          enabled: true
          opacity: 100%
        plot:
          type: plot
          cap: round
          gradient: TemperatureGradient
        resolution: 2
    - figure: precipitation
      margins:
        bottom: 30px
        top: 50%
      environment.precipitation.precipitation:
        plot:
          type: plot
          cap: round
          gradient: PrecipitationRateGradient
          weight: 0.3
      max: 0.5
    - figure: lightIrradiance
      margins:
        bottom: 30px
        top: 75%
      environment.light.irradiance.irradiance:
        plot:
          type: plot
          cap: round
          gradient: FabledSunsetGradientWattsPerSquareMeter
          weight: 0.3
      max: 840
    - figure: windSpeed
      margins:
        bottom: 30px
        top: 70%
      environment.wind.speed.speed:
        plot:
          type: plot
          cap: round
          dashPattern: 2, 4
          gradient: RipeMalinkaGradient
          opacity: 0.7
          weight: 0.3
      max: 10
