- type: graph
  geometry:
    height: 479.057px
    width: 618.9px
    x: 378.1px
    y: 0px
  timeframe:
    days: 2
    hours: 12
  figures:
    - figure: temperature
      environment.temperature.dewpoint:
        labeled: false
        plot:
          color: 5882ff
          dashPattern: 5, 5
          opacity: 0.9
          scalar: 0.322
          type: plot
      environment.temperature.feelsLike:
        labeled: false
        plot:
          gradient: &id001 TemperatureGradient
          scalar: 0.433
          type: plot
      environment.temperature.temperature:
        labeled: true
        plot:
          gradient: *id001
          scalar: 0.866
          type: plot
      margins: 0%, 24.6%, 0%, 11.8%
    - figure: precipitation
      environment.precipitation.precipitation:
        labeled: false
        plot:
          gradient: PrecipitationRateGradient
          scalar: 0.3
          type: plot
      margins: 0%, 50%, 0%, 10%
      max: 0.5
    - figure: precipitationProbability
      environment.precipitation.probability:
        labeled: false
        plot:
          dashPattern: 2, 3
          gradient: PrecipitationProbabilityGradient
          scalar: 0.2
          type: plot
      margins: 0%, 60%, 0%, 10%
      max: 100
    - figure: windSpeed
      environment.wind.speed.speed:
        labeled: false
        plot:
          color: ff2f20
          dashPattern: 2, 4
          opacity: 0.7
          scalar: 0.3
          type: plot
      margins: 0%, 75%, 0%, 10%
- type: clock
  geometry:
    height: 22.2%
    width: 36.5%
    x: 0%
    y: 1.7%
  frozen: true
  items:
    - format: '%A, %B %-d'
      geometry:
        height: 27.1%
        width: 100%
        x: 0%
        y: 0%
      filters:
        - 0Add Ordinal
      alignment: CenterRight
    - type: moon
      geometry:
        height: 44.1%
        width: 19%
        x: 79.2%
        y: 25%
      locked: true
    - format: '%-I:%M'
      geometry:
        height: 70%
        width: 77.3%
        x: 0%
        y: 27.2%
      alignment: CenterRight
    - format: '%p'
      geometry:
        height: 26.5%
        width: 20.5%
        x: 77%
        y: 68%
      filters:
        - 1Lower
