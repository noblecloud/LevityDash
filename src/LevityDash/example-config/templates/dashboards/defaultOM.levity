- type: graph
  geometry:
    height: 479.057px
    width: 653.76px
    x: 343.24px
    y: 0px
  timeframe:
    days: 1
    hours: 12
  figures:
    - environment.temperature.dewpoint:
        labeled: false
        plot:
          color: 5882ff
          dashPattern: 5, 5
          opacity: 0.9
          scalar: 0.322
          type: plot
      environment.temperature.feelsLike:
        labeled: false
        plot:
          gradient: TemperatureGradient
          scalar: 0.433
          type: plot
      environment.temperature.temperature:
        labeled: true
        plot:
          gradient: TemperatureGradient
          scalar: 0.866
          type: plot
      margins: 0%, 24.6%, 0%, 11.8%
    - environment.wind.speed.speed:
        labeled: false
        plot:
          color: ff2f20
          dashPattern: 1, 1
          opacity: 0.7
          scalar: 0.4
          type: plot
      margins: 0%, 75%, 0%, 10%
- type: clock
  geometry:
    height: 22.2%
    width: 36.5%
    x: 0%
    y: 1.7%
  frozen: true
  items:
    - format: '%A, %B %-d'
      geometry:
        height: 27.1%
        width: 100%
        x: 0%
        y: 0%
      filters:
        - 0Add Ordinal
      alignment: CenterRight
    - type: moon
      geometry:
        height: 44.1%
        width: 19%
        x: 79.2%
        y: 25%
      locked: true
    - format: '%-I:%M'
      geometry:
        height: 70%
        width: 77.3%
        x: 0%
        y: 27.2%
      alignment: CenterRight
    - format: '%p'
      geometry:
        height: 26.5%
        width: 20.5%
        x: 77%
        y: 68%
      filters:
        - 1Lower
- type: group
  geometry:
    height: 44.1%
    width: 23.2%
    x: 0%
    y: 21.3%
  items:
    - type: realtime.text
      key: environment.temperature.temperature
      display:
        value:
          alignment: CenterRight
          margins: 0%, 0%, 0%, 0%
      title: false
      geometry:
        height: 58.7%
        width: 100%
        x: 0%
        y: 0%
    - type: realtime.text
      key: environment.humidity.humidity
      display:
        value:
          margins: 0%, 0%, 0%, 0%
